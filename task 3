def передостанній_символ(рядок):
    if len(рядок) < 2:
        return "Рядок занадто короткий, немає передостаннього символу"
    if len(рядок) == 2:
        передостанній = рядок[0]  # Перший символ, якщо рядок має довжину 2
    else:
        передостанній = рядок[-2]  # Передостанній символ для решти випадків
    return передостанній

ваш_рядок = "Ваш рядок"
результат = передостанній_символ(ваш_рядок)
print("Передостанній символ рядка:", результат)
